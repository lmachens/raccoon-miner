{
  "name": "raccoon-miner",
  "private": true,
  "version": "0.21.0",
  "description": "Crypto mining app for Overwolf appstore",
  "dependencies": {
    "@material-ui/core": "1.4.2",
    "@material-ui/icons": "2.0.0",
    "classnames": "2.2.6",
    "lodash": "4.17.10",
    "prop-types": "15.6.2",
    "prune": "0.0.2",
    "raven-for-redux": "1.3.1",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-ga": "2.5.3",
    "react-redux": "5.0.7",
    "recompose": "0.28.1",
    "redux": "4.0.0",
    "redux-persist": "5.10.0",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.56",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.56",
    "@babel/preset-env": "7.0.0-beta.56",
    "@babel/preset-react": "7.0.0-beta.56",
    "@storybook/react": "3.4.10",
    "archiver": "2.1.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.2",
    "babel-loader": "7.1.5",
    "conventional-changelog-angular": "5.0.0",
    "conventional-github-releaser": "3.1.2",
    "eslint": "5.2.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.18.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.10.0",
    "gh-release-assets": "1.1.2",
    "github-remove-all-releases": "1.0.1",
    "husky": "0.14.3",
    "jest": "23.4.2",
    "lint-staged": "7.2.0",
    "prettier": "1.14.0",
    "regenerator-runtime": "0.12.1",
    "request": "2.87.0",
    "rollup": "0.63.5",
    "rollup-plugin-babel": "4.0.0-beta.7",
    "rollup-plugin-commonjs": "9.1.4",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-terser": "1.0.1",
    "rollup-watch": "4.3.1",
    "standard-version": "4.4.0"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "production": "rollup -c production.rollup.config.js",
    "test": "jest",
    "release": "git checkout master && git merge dev && standard-version && node tools/release && git checkout dev && git merge master && git push",
    "precommit": "lint-staged",
    "prepush": "jest",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "standard-version": {
    "scripts": {
      "postcommit": "node tools/syncVersion && rollup -c production.rollup.config.js && rollup -c && git add . && git commit -am \"chore(overwolf): manifest version bump\"",
      "posttag": "git push --follow-tags origin master"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lmachens/raccoon-miner.git"
  },
  "keywords": [
    "overwolf",
    "crypto",
    "mining",
    "cryptocurrency",
    "ethereum"
  ],
  "author": "Leon Machens",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/lmachens/raccoon-miner/issues"
  },
  "homepage": "https://github.com/lmachens/raccoon-miner"
}
