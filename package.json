{
  "name": "raccoon-miner",
  "private": true,
  "version": "0.1.0",
  "description": "Crypto mining app for Overwolf appstore",
  "dependencies": {
    "@material-ui/core": "1.0.0",
    "@material-ui/icons": "1.0.0",
    "classnames": "2.2.5",
    "lodash": "4.17.10",
    "prop-types": "15.6.1",
    "prune": "0.0.2",
    "raven-for-redux": "^1.3.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-ga": "2.5.2",
    "react-redux": "5.0.7",
    "recompose": "0.27.0",
    "redux": "4.0.0",
    "redux-persist": "5.9.1",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.47",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.47",
    "@babel/preset-env": "7.0.0-beta.47",
    "@babel/preset-react": "7.0.0-beta.47",
    "@storybook/react": "3.4.5",
    "archiver": "2.1.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "23.0.0-charlie.2",
    "babel-loader": "7.1.4",
    "conventional-changelog-angular": "3.0.7",
    "conventional-github-releaser": "2.0.2",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jest": "21.15.1",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.8.2",
    "gh-release-assets": "1.1.2",
    "github-remove-all-releases": "1.0.1",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "lint-staged": "7.1.0",
    "prettier": "1.12.1",
    "regenerator-runtime": "0.11.1",
    "request": "2.86.0",
    "rollup": "0.58.2",
    "rollup-plugin-babel": "4.0.0-beta.4",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "3.0.0",
    "rollup-watch": "4.3.1",
    "standard-version": "4.3.0"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "production": "rollup -c production.rollup.config.js",
    "test": "jest",
    "release": "standard-version && node tools/release",
    "precommit": "lint-staged",
    "prepush": "jest",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "standard-version": {
    "scripts": {
      "postcommit": "node tools/syncVersion && git commit -am \"chore(overwolf): manifest version bump\"",
      "posttag": "git push --follow-tags origin master"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lmachens/raccoon-miner.git"
  },
  "keywords": [
    "overwolf",
    "crypto",
    "mining",
    "cryptocurrency",
    "ethereum"
  ],
  "author": "Leon Machens",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lmachens/raccoon-miner/issues"
  },
  "homepage": "https://github.com/lmachens/raccoon-miner"
}
